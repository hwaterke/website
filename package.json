{
  "name": "hwaterke-website",
  "description": "Harold Waterkeyn website",
  "version": "1.0.0",
  "author": "Harold Waterkeyn",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "lint": "eslint src",
    "format": "prettier --write 'src/**/*.js'",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "bulma": "^0.6.2",
    "gatsby": "^1.9.203",
    "gatsby-link": "^1.6.37",
    "gatsby-plugin-react-helmet": "^2.0.5",
    "gatsby-plugin-sass": "^1.0.17",
    "gatsby-plugin-styled-components": "^2.0.7",
    "react-helmet": "^5.2.0",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.18.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.45.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "prettier": "^1.10.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote --bracket-spacing=false --no-semi",
      "eslint --max-warnings 0",
      "git add"
    ],
    "*.{css,scss,json,md}": ["prettier --write", "git add"]
  }
}
